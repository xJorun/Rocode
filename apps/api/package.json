{
  "name": "@rocode/api",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format cjs --dts --tsconfig tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@rocode/shared": "*",
    "@sentry/node": "^10.30.0",
    "@sentry/profiling-node": "^10.30.0",
    "bullmq": "^5.25.5",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.36.4",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-winston": "^4.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jose": "^5.9.6",
    "jsdom": "^27.3.0",
    "marked": "^15.0.3",
    "nodemailer": "^7.0.11",
    "pg": "^8.13.1",
    "rate-limit-redis": "^4.2.0",
    "stripe": "^17.3.1",
    "uuid": "^11.0.3",
    "winston": "^3.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.28.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
